@article{bobb2020,
	title = {Co-activation of the {L2} during {L1} auditory processing: {An} {ERP} cross-modal priming study},
	volume = {203},
	issn = {0093-934X},
	shorttitle = {Co-activation of the {L2} during {L1} auditory processing},
	url = {http://www.sciencedirect.com/science/article/pii/S0093934X19303554},
	doi = {10.1016/j.bandl.2019.104739},
	abstract = {Several studies have shown that unbalanced bilinguals activate both of their languages simultaneously during L2 processing; however, evidence for L2 activation while participants are tested exclusively in their L1 has been more tenuous. Here, we investigate whether bilingual participants implicitly activate the label for a picture in their two languages, and whether labels activated in L2 can prime activation of cross-linguistically related L1 lexical targets. We tested highly proficient early Spanish-Basque bilinguals on an ERP cross-modal priming task conducted only in their L1, Spanish. Participants activated prime picture labels in both Spanish and Basque. More importantly, participants activated Basque translations of Spanish auditory targets, even in a Spanish experimental environment with no reference to Basque. Results provide strong evidence for non-selective bilingual lexical access, showing co-activation extending to lexical levels beyond phonological overlap. Our results add to the growing body of evidence for the interconnective nature of bilingual language activation.},
	language = {en},
	urldate = {2020-06-29},
	journal = {Brain and Language},
	author = {Bobb, Susan C. and Von Holzen, Katie and Mayor, Julien and Mani, Nivedita and Carreiras, Manuel},
	month = apr,
	year = {2020},
	keywords = {Basque, Bilingualism, Event-related potentials, Language co-activation, Priming, Spanish},
	pages = {104739}
}

@article{kroll1994,
	title = {Category interference in translation and picture naming: {Evidence} for asymmetric connection between bilingual memory representations},
	volume = {33},
	issn = {1096-0821(Electronic),0749-596X(Print)},
	shorttitle = {Category interference in translation and picture naming},
	doi = {10.1006/jmla.1994.1008},
	abstract = {In Exp 1 (16 native-English-speaking undergraduates [NESUs]) and Exp 3 (24 Dutch–English bilingual undergraduates), picture naming (PN) and bilingual translation (BLT) were slower in the categorized than in the randomized list conditions. In Exp 2 (16 NESUs) this category interference (CI) effect in PN was eliminated when PN alternated with word naming. This suggests that in both PN and BLT a conceptual representation is used to retrieve a lexical entry. When conceptual activity is sufficiently great to activate a multiple set of corresponding lexical representations, interference is produced. In Exp 3 CI in BLT occurred only when translation was performed from the 1st to the 2nd language, suggesting that the 2 directions of translation engage different interlanguage connections. A model to account for the asymmetric mappings of words to concepts in bilingual memory is described. (PsycINFO Database Record (c) 2017 APA, all rights reserved)},
	number = {2},
	journal = {Journal of Memory and Language},
	author = {Kroll, Judith F. and Stewart, Erika},
	year = {1994},
	note = {Place: Netherlands
Publisher: Elsevier Science},
	keywords = {Bilingualism, Interference (Learning), Lexical Access, Memory, Naming, Pictorial Stimuli},
	pages = {149--174}
}

