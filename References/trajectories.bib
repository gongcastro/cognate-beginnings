	shorttitle = {Variability and stability in early language acquisition},
	url = {https://www.cambridge.org/core/journals/bilingualism-language-and-cognition/article/variability-and-stability-in-early-language-acquisition-comparing-monolingual-and-bilingual-infants-speech-perception-and-word-recognition/6DCC3BF43ACE25F05244A92CB7E10D32},
