@article{nelson1973structure,
  title = {Structure and {{Strategy}} in {{Learning}} to {{Talk}}},
  author = {Nelson, Katherine},
  year = {1973},
  journal = {Monographs of the Society for Research in Child Development},
  volume = {38},
  number = {1/2},
  pages = {1--135},
  publisher = {{[Society for Research in Child Development, Wiley]}},
  issn = {0037-976X},
  doi = {10.2307/1165788},
  abstract = {This "Monograph" reports the results of a longitudinal study of the acquisition of first words by 18 children between 1 and 2 years of age and discusses them in terms of an interaction model of the language-learning process. The first 50 words acquired by each child are analyzed in terms of grammatical form, content, and semantic structure. In general, children began by naming objects exhibiting salient properties of change whether as the result of the child's own action (e. g., "ball") or independent of it (e. g., "car"). They learned words of all major, grammatical form classes and tended to differentiate small concept domains-for example, food words or animals-from the beginning. Children differed in the functional type of vocabulary (referential or expressive) acquired and in the length of their initial speech units as well as in the content-the particular referents they chose to name. These differences were related to patterns of language acquisition; for example, referential speakers built larger vocabularies more quickly than did expressive speakers. Certain strategies used by children for acquiring first words-comprehension, production, imitation, repetition, and questioning-were found to be related to language progress. For example, comprehension at 15 months was positively related to all later indices of language maturity, while spontaneous imitation at 24 months was indicative of immature speech. A non-directive parental strategy that was accepting of the child's behavior-both verbal and nonverbal-facilitated the child's progress in language acquisition. In general, mothers gave very little contingent feedback to their language-learning children and showed very little expansion of utterances. All mothers, however, used short (three to four words) sentences to them. Environmental factors that were associated with the child's language progress-for example, many outings, exposure to many adults, little T. V. -appeared to be a function of facilitating mother attitudes. An interaction model that interrelates the child's preverbal concepts, his acquisition strategies, and parental-acceptance patterns is presented as a framework for understanding these findings. Acquisition histories of individual children illustrate the way in which these factors interact.},
  file = {C\:\\Users\\u155880\\Zotero\\storage\\H98W3DN9\\Nelson - 1973 - Structure and Strategy in Learning to Talk.pdf}
}

@article{novogrodskyundefined/edage,
