@article{bergelson2020comprehension,
  title = {The {{Comprehension Boost}} in {{Early Word Learning}}: {{Older Infants Are Better Learners}}},
  shorttitle = {The {{Comprehension Boost}} in {{Early Word Learning}}},
  author = {Bergelson, Elika},
  year = {2020},
  journal = {Child Development Perspectives},
  volume = {14},
  number = {3},
  pages = {142--149},
  issn = {1750-8606},
  doi = {10.1111/cdep.12373},
  abstract = {Recent research has revealed that infants begin understanding words at around 6 months. After that, infants' comprehension vocabulary increases gradually in a linear way over 8\textendash 18 months, according to data from parental checklists. In contrast, infants' word comprehension improves robustly, qualitatively, and in a nonlinear way just after their first birthday, according to data from studies on spoken word comprehension. In this review, I integrate observational and experimental data to explain these divergent results. I argue that infants' comprehension boost is not well-explained by changes in their language input for common words, but rather by proposing that they learn to take better advantage of relatively stable input data. Next, I propose potentially complementary theoretical accounts of what makes older infants better learners. Finally, I suggest how the research community can expand our empirical base in this understudied area, and why doing so will inform our knowledge about child development.},
  langid = {english},
  keywords = {infancy,language development,language input,word learning},
  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/cdep.12373},
  file = {C\:\\Users\\u155880\\Zotero\\storage\\3KEWF2BQ\\Bergelson - 2020 - The Comprehension Boost in Early Word Learning Ol.pdf}
}

