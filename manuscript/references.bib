@incollection{patterson2004bilingual,
  title = {Bilingual {{Lexical Development}}: {{Influences}}, {{Contexts}}, and {{Processes}}},
  shorttitle = {Bilingual {{Lexical Development}}},
  booktitle = {Bilingual Language Development and Disorders in {{Spanish-English}} Speakers},
  author = {Patterson, Janet L. and Pearson, Barbara Zurer},
